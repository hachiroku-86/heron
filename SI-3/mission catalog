SET NAMES utf8;
SET time_zone = '+00:00';
SET foreign_key_checks = 0;
SET sql_mode = 'NO_AUTO_VALUE_ON_ZERO';

DROP TABLE IF EXISTS `M2_COLLECTION`;
CREATE TABLE `M2_COLLECTION` (
  `NUMCOLLECTION` int(11) NOT NULL,
  `DATELANCEMENT` date DEFAULT NULL,
  `NOMCOLLECTION` varchar(30) DEFAULT NULL,
  `HARMONIE` varchar(30) DEFAULT NULL,
  PRIMARY KEY (`NUMCOLLECTION`),
  KEY `Petit_chat` (`NUMCOLLECTION`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

