DROP TABLE IF EXISTS `M2_PRODUIT`;
CREATE TABLE `M2_PRODUIT` (
  `REFP` char(4) NOT NULL,
  `DESIGN` varchar(30) DEFAULT NULL,
  `COULEUR` varchar(30) DEFAULT NULL,
  `DIMMENSSION` varchar(30) DEFAULT NULL,
  `PRIXHT` double DEFAULT NULL,
  `NUMCOLLECTION` int(11) DEFAULT NULL,
  PRIMARY KEY (`REFP`),
  KEY `fkNUMCOLLECTION` (`NUMCOLLECTION`),
  CONSTRAINT `fkNUMCOLLECTION` FOREIGN KEY (`NUMCOLLECTION`) REFERENCES `M2_COLLECTION` (`NUMCOLLECTION`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

